	j main
	
facto:	sw $ra, -4($sp)#sauvegarde des données courantes
	addi $sp, $sp, -4
	
	li $v0, 1
	move $t0, $a0#variable n
loop:	beqz  $t0, end
	mul $v0, $t0, $v0
	addi $t0, $t0, -1
	j loop
	
end:	lw $ra, 0($sp)#restitution des données courantes
	addi $sp, $sp, 4
	jr $ra

main:	li $a0, 3
	jal facto
	la $a0, ($v0)
	li $v0, 1
	syscall